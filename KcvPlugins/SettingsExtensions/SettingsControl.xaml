<UserControl x:Class="AMing.SettingsExtensions.SettingsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:amingdata="clr-namespace:AMing.SettingsExtensions.Data"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel DockPanel.Dock="Bottom"
										Margin="10,6">
            <TextBlock Text="Binding Resources.Dockyard_Display_Title, Source={x:Static models:ResourceService.Current}, Mode=OneWay}"
					Style="{DynamicResource HeaderTextStyleKey}" />
            <CheckBox Margin="8,6"
					Content="asdasd"
					IsChecked="{Binding Enable_ExitTip, Source={x:Static amingdata:Settings.Current}, Mode=OneWay}"  />
        </StackPanel>
    </Grid>
</UserControl>
